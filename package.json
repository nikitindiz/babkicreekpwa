  "private": true,
  "homepage": "https://nikitindiz.github.io/babkicreekpwa",
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^17.0.10",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.7",
    "is-mobile": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "normalize.css": "^8.0.1",
    "qr-encode": "^0.3.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-intl": "^5.25.1",
    "react-redux": "^8.1.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "use-debounce": "^10.0.0",
    "use-sync-external-store": "^1.2.0",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.4.2",
    "workbox-broadcast-update": "^6.4.2",
    "workbox-cacheable-response": "^6.4.2",
    "workbox-core": "^6.4.2",
    "workbox-expiration": "^6.4.2",
    "workbox-google-analytics": "^6.4.2",
    "workbox-navigation-preload": "^6.4.2",
    "workbox-precaching": "^6.4.2",
    "workbox-range-requests": "^6.4.2",
    "workbox-routing": "^6.4.2",
    "workbox-strategies": "^6.4.2",
    "workbox-streams": "^6.4.2"
  },
  "scripts": {
    "start": "cross-env craco start",
    "start-https": "cross-env HTTPS=true craco start",
    "predeploy": "craco build",
    "deploy": "gh-pages -d build",
    "test": "craco test",
    "eject": "craco eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint-n-fix": "eslint src/**/*.ts{,x} --fix",
    "extract-translations": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file src/lang/en-extracted.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile-translations": "formatjs compile src/lang/en-extracted.json --out-file src/lang/en.json"
  },
  "jest": {
    "moduleNameMapper": {
      "types/(.+)": "<rootDir>/src/$1",
      "context/(.+)": "<rootDir>/src/context/$1",
      "models/(.+)": "<rootDir>/src/models/$1",
      "utils/(.+)": "<rootDir>/src/utils/$1",
      "store/(.+)": "<rootDir>/src/store/$1",
      "components/(.+)": "<rootDir>/src/components/$1",
      "containers/(.+)": "<rootDir>/src/containers/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^6.1.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.3"
  }
}
